<!-- 만든이: 김하겸 -->
<!-- item_id mapper는 사업자(vender)가 등록한 업체에 대한 처리입니다 -->
<mapper namespace="com.reservation.dao.IMapDao">
	<select id="selectPlace" parameterType="String" resultType="map">
		SELECT *
		FROM placedetail
		WHERE place_name LIKE '%새미%';
	</select>
</mapper>