<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<!-- 만든이: 김하겸 -->
<!-- item_id mapper는 사업자(vender)가 등록한 업체에 대한 처리입니다 -->
<mapper namespace="com.reservation.dao.IMapDao">
<!--   	해당 기능에 필요한 테이블엔 업체명,업체관련사진,업체장소 데이터가 필수로 들어가야된다 -->
<!--   	그러기위에선 현재 데이터베이스의 수정이 필요함 -->
	<select id="selectAllMyItems" parameterType="String"
		resultType="map">
		SELECT *
		FROM PLACEDETAIL
		WHERE service_name LIKE CONCAT('%', #{query}, '%')

	</select>

</mapper>